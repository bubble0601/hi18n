{
  "name": "@hi18n/eslint-plugin",
  "version": "0.1.0",
  "description": "Stateless message catalog for internationalization -- eslint plugin",
  "main": "dist/index.js",
  "repository": "https://github.com/qnighy/hi18n",
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "babel": "babel -x .ts src -d dist --source-maps --ignore '**/*.test.ts'",
    "build": "yarn babel && yarn tsc",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.ts' .eslintrc.js babel.config.js",
    "test": "jest",
    "tsc": "tsc --build"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.17.9",
    "eslint-visitor-keys": "^3.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^28.0.1",
    "@types/eslint": "^8.4.1",
    "@types/estree": "^0.0.51",
    "@types/estree-jsx": "^0.0.1",
    "babel-jest": "^28.0.1",
    "eslint": "^8.14.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  }
}
