{
  "name": "@hi18n/eslint-plugin",
  "version": "0.1.0",
  "description": "Message internationalization meets immutability and type-safety - ESLint plugin",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "format",
    "formatting",
    "translate",
    "translation",
    "hi18n",
    "eslint",
    "eslintplugin"
  ],
  "homepage": "https://github.com/wantedly/hi18n#readme",
  "bugs": {
    "url": "https://github.com/wantedly/hi18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wantedly/hi18n.git",
    "directory": "packages/eslint-plugin"
  },
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "files": [
    "dist/**/*",
    "!dist/*.tsbuildinfo",
    "src/**/*",
    "!src/**/*.test.ts"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "babel": "babel -x .ts src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts'",
    "build": "yarn babel && yarn tsc",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.ts' .eslintrc.js babel.config.js",
    "test": "jest",
    "tsc": "tsc --build"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.17.9",
    "eslint-visitor-keys": "^3.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^28.0.3",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.2",
    "@types/estree": "^0.0.51",
    "@types/estree-jsx": "^0.0.1",
    "@typescript-eslint/types": "^5.22.0",
    "@typescript-eslint/utils": "^5.22.0",
    "babel-jest": "^28.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "espree": "^9.3.1",
    "jest": "^28.0.3",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
